{
  "name": "node-ts-starter",
  "version": "1.0.0",
  "description": "Building your app with Node",
  "main": "./dist/server.js",
  "scripts": {
    "dev": "nodemon -w src -e ts --exec \"ts-node -r tsconfig-paths/register src/server.ts\"",
    "build": "tsc && tsc-alias",
    "start": "node dist/server.js"
  },
  "keywords": [
    "JavaScript",
    "Node",
    "Express",
    "Mongoose"
  ],
  "author": "360Pawan",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cloudinary": "^1.41.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "googleapis": "^131.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.5.0",
    "mongoose-aggregate-paginate-v2": "^1.1.1",
    "multer": "1.4.5-lts.1",
    "nodemailer": "^6.9.14"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.11",
    "@types/nodemailer": "^6.4.15",
    "dotenv": "^16.4.5",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
