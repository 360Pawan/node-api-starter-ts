{
  "name": "node-ts-starter",
  "version": "1.0.0",
  "description": "Building your app with Node",
  "main": "./dist/server.js",
  "scripts": {
    "dev": "nodemon -w src -e ts --exec \"ts-node -r tsconfig-paths/register src/server.ts\"",
    "build": "tsc && tsc-alias",
    "start": "node dist/server.js"
  },
  "keywords": [
    "JavaScript",
    "Node",
    "Express",
    "Mongoose"
  ],
  "author": "360Pawan",
  "license": "ISC",
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "mongoose": "^8.5.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "dotenv": "^16.4.5",
    "nodemon": "^3.1.4",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.5.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
